{
  "label": "Weather Forecast",
  "languages": [
    "en"
  ],
  "links": {},
  "contentTypes": {
    "Weather": {
      "contentType": [],
      "properties": {
        "Location": {
          "type": "Location"
        },
        "Description": {
          "type": "String"
        },
        "Temperature": {
          "type": "Int"
        }
